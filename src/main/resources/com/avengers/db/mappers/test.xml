<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.avengers.test.TestVO">

	<!-- id의 경우에는 Mapper.java에 동일하게 사용해야된다. -->
	<select id="getTestList" resultType="com.avengers.test.TestVO">
		SELECT * FROM SEqurity
	</select>
	
	<select id="getTest" parameterType="com.avengers.test.TestVO" resultType="com.avengers.test.TestVO">
		select * from SEqurity where id=#{id} and pwd= #{pwd}
	</select>
	
</mapper>