<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lr">
	<!-- 학과공지게시판을 구하기위해 학과이름을 구하는 구문이다. -->
	<select id="selectLr_hour" parameterType="java.lang.String" resultType="com.avengers.db.dto.LrVO">
		SELECT LR_HOUR FROM LR WHERE LR_LCT=#{lct_num}  
	</select>
	
	
	
</mapper>
