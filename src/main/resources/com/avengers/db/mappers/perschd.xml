<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="perschd">

	<!-- id의 경우에는 Mapper.java에 동일하게 사용해야된다. -->
	<select id="selectPerschdList" parameterType="com.avengers.db.dto.PerschdVO" resultType="com.avengers.db.dto.PerschdVO">
		SELECT 
			PERSCHD_TITLE,
			PERSCHD_CONT,
			PERSCHD_DATE,
			PERSCHD_START_DATE,
			PERSCHD_END_DATE,
			PERSCHD_WRITER
		FROM PERSCHD
		WHERE PERSCHD_WRITER = #{perschd_writer}
	</select>
	
	
</mapper>