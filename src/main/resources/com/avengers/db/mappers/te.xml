<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="te">
	<!-- 응시여부 업데이트 구문 -->
	<update id="updateTeCheck" parameterType="java.lang.String">
		UPDATE TE
		SET TE_CHECK = '1' 
		WHERE TE_NUM=#{te_num}
	</update>
	
</mapper>